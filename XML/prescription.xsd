 <?xml version="1.0" encoding="ISO-8859-1"?>
<Schema xmlns="urn:schemas-microsoft-com:xml-data" xmlns:dt="urn:schemas-microsoft-com:datatypes">
 
 <ElementType name="prescription" content="mixed">
   <group minOccurs="0" maxOccurs="*">
      <element type="Record"/>
   </group>
 </ElementType>

 <ElementType name="Record" content="eltOnly" order="seq">
     <element type="rx_id"/>
     <element type="start_date"/>
     <element type="end_date"/>
     <element type="dose"/>
     <element type="frequency"/>
     <element type="PHN"/>
     <element type="prescriber_id"/>
     <element type="d_id"/>
 </ElementType>

  <ElementType name="rx_id" content="textOnly"/>
  <ElementType name="start_date" content="textOnly"/>
  <ElementType name="end_date" content="textOnly"/>
  <ElementType name="dose" content="textOnly"/>
  <ElementType name="frequency" content="textOnly"/>
  <ElementType name="PHN" content="textOnly"/>
  <ElementType name="prescriber_id" content="textOnly"/>
  <ElementType name="d_id" content="textOnly"/>

</Schema>